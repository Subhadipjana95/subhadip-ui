{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "froniq-ui",
  "homepage": "https://froniq-ui.com",
  "items": [
    {
      "name": "button",
      "type": "registry:ui",
      "title": "Button",
      "description": "Displays a button or a component that looks like a button.",
      "dependencies": [
        "@radix-ui/react-slot"
      ],
      "registryDependencies": ["utils"],
      "files": [
        {
          "path": "components/button/button.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "utils",
      "type": "registry:lib",
      "title": "Utils",
      "description": "Utility functions for Tailwind CSS.",
      "dependencies": [
        "clsx",
        "tailwind-merge"
      ],
      "files": [
        {
          "path": "lib/utils.tsx",
          "type": "registry:lib"
        }
      ]
    }
  ]
}